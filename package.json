{
  "name": "goindex",
  "version": "1.0.0",
  "description": "Index page for Google Drive",
  "main": "dist/index.js",
  "scripts": {
    "start": "rm -rf dist && cross-env NODE_ENV=development webpack -w --progress",
    "build": "rm -rf dist && cross-env NODE_ENV=production webpack -p --progress",
    "lint": "yarn run lint:es && yarn run lint:prettier",
    "lint:es": "eslint --ext .js,.ts ./ --cache --quiet",
    "lint:prettier": "prettier --list-different \"**/*.{js,ts,json,yml,yaml,md}\"",
    "fix": "yarn run fix:es && yarn run fix:prettier",
    "fix:es": "eslint --ext .js,.ts ./ --cache --quiet --fix",
    "fix:prettier": "prettier --write \"**/*.{js,ts,json,yml,yaml,md}\""
  },
  "repository": "https://github.com/VenDream/goindex",
  "author": "VenDream <yeshenxue@qq.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/node": "^13.13.0",
    "@types/webpack": "^4.41.12",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6",
    "prettier": "^2.0.4",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
